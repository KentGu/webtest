<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="../../dataBean.xsl"?>
<!--suppress ALL -->
<dataBean xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude"
          xsi:noNamespaceSchemaLocation="../../../../../../cobiz/src/main/resources/schemas/dataBean.xsd">
    <xi:include href="../system/default.xml" xpointer="element(/1/1)"/>
    <xi:include href="../feature/reports.xml" xpointer="element(/1/1)"/>
    <scenarioData>
        <interestRate id="wht.rate.exist">
            <interestRateName id="audit.wht.rate.create">audit_wht_rate</interestRateName>
        </interestRate>

        <interestRate id="wht.rate.update">
            <description id="des" random="true"/>
            <refixFrequency>Weekly</refixFrequency>
            <status>enabled</status>
            <type>Interest</type>
            <dailyInterestRate>
                <date>10/10/2016</date>
                <rate>10</rate>
            </dailyInterestRate>
            <dailyInterestRate remove="true">
                <date>10/10/2016</date>
                <rate>10</rate>
            </dailyInterestRate>

        </interestRate>


        <auditInterestRatesReportFilter id="audit.wht.rates.by.name">
            <rateName ref="audit.wht.rate.create"/>
            <startDate>t-1000</startDate>
        </auditInterestRatesReportFilter>
        <variable id="auditReportPath"/>
        <reportValidationRules id="audit.wht.report.rule">
            <reportValidationRule>
                <operator>&gt;=</operator>
                <count>1</count>
                <field>
                    <fieldName>Interest Rate</fieldName>
                    <value>audit_wht_rate</value>
                </field>
                <field>
                    <fieldName>Operation</fieldName>
                    <value>Modify</value>
                </field>
                <field>
                    <fieldName>Attribute</fieldName>
                    <value>Description</value>
                </field>
                <field>
                    <fieldName>Value Before</fieldName>
                    <value>audit_wht_rate</value>
                </field>
                <field>
                    <fieldName>Value After</fieldName>
                    <value ref="des" />
                </field>
            </reportValidationRule>
            <reportValidationRule>
                <operator>&gt;=</operator>
                <count>1</count>
                <field>
                    <fieldName>Interest Rate</fieldName>
                    <value>audit_wht_rate</value>
                </field>
                <field>
                    <fieldName>Operation</fieldName>
                    <value>Modify</value>
                </field>
                <field>
                    <fieldName>Attribute</fieldName>
                    <value>RateType</value>
                </field>
                <field>
                    <fieldName>Value Before</fieldName>
                    <value>WHT</value>
                </field>
                <field>
                    <fieldName>Value After</fieldName>
                    <value>Interest</value>
                </field>
            </reportValidationRule>
            <reportValidationRule>
                <operator>&gt;=</operator>
                <count>1</count>
                <field>
                    <fieldName>Interest Rate</fieldName>
                    <value>audit_wht_rate</value>
                </field>
                <field>
                    <fieldName>Operation</fieldName>
                    <value>Modify</value>
                </field>
                <field>
                    <fieldName>Attribute</fieldName>
                    <value>RefixFreq</value>
                </field>
                <field>
                    <fieldName>Value Before</fieldName>
                    <value>Daily</value>
                </field>
                <field>
                    <fieldName>Value After</fieldName>
                    <value>Weekly</value>
                </field>
            </reportValidationRule>
            <reportValidationRule>
                <operator>&gt;=</operator>
                <count>1</count>
                <field>
                    <fieldName>Interest Rate</fieldName>
                    <value>audit_wht_rate</value>
                </field>
                <field>
                    <fieldName>Operation</fieldName>
                    <value>Create</value>
                </field>
                <field>
                    <fieldName>Attribute</fieldName>
                    <value>Rate</value>
                </field>
                <field>
                    <fieldName>Value Before</fieldName>
                    <value></value>
                </field>
                <field>
                    <fieldName>Value After</fieldName>
                    <value>10.0</value>
                </field>
            </reportValidationRule>
            <reportValidationRule>
                <operator>&gt;=</operator>
                <count>1</count>
                <field>
                    <fieldName>Interest Rate</fieldName>
                    <value>audit_wht_rate</value>
                </field>
                <field>
                    <fieldName>Operation</fieldName>
                    <value>Delete</value>
                </field>
                <field>
                    <fieldName>Attribute</fieldName>
                    <value>Rate</value>
                </field>
                <field>
                    <fieldName>Value Before</fieldName>
                    <value>10.0</value>
                </field>
                <field>
                    <fieldName>Value After</fieldName>
                    <value></value>
                </field>
            </reportValidationRule>

        </reportValidationRules>
    </scenarioData>
</dataBean>
